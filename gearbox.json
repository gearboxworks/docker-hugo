{
	"schema": "gear-1",
	"meta": {
		"state": "production",
		"organization": "gearboxworks",
		"name": "hugo",
		"label": "Hugo",
		"maintainer": "Gearbox Team <team@gearbox.works>",
		"class": "webserver",
		"refurl": "https://gohugo.io/"
	},
	"build": {
		"ports": {

			"http": "80",
			"hugo": "1313"
		},
		"run": "",
		"args": "",
		"env": {

			"HUGO_PUBLISHDIR": "/home/gearbox/projects/default/www",
			"HUGO_SRCDIR": "/home/gearbox/projects/default/hugo",
			"HUGO_USER": "gearbox"
		},
		"network": "--network gearboxnet",
		"volumes": "",
		"restart": "--restart no"
	},
	"run": {
		"commands": {
			"default": "/usr/local/bin/hugo",
			"hugo": "/usr/local/bin/hugo",
			"hugo-extended": "/usr/local/bin/hugo-extended",
			"minify": "/usr/local/bin/minify"
		}
	},
